{ dup length cp add rmargin gt{nl/cp 0 def}if
  dup length cp add/cp exch def prnt
} readonly def
/cvsprint     {=string cvs tprint( )tprint} readonly def
/unknowntype  {exch pop cvlit(??)tprint cvsprint} readonly def
